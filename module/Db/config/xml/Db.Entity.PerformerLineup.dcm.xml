<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\PerformerLineup">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="note" type="text" nullable="true"/>
    <many-to-one field="performer" target-entity="Db\Entity\Performer" inversed-by="performerLineup" fetch="EAGER">
      <join-columns>
        <join-column name="performer_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lineup" target-entity="Db\Entity\Lineup" inversed-by="performerLineup" fetch="EAGER">
      <join-columns>
        <join-column name="lineup_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
